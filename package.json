{
  "name": "next-authentication",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "tsc && eslint \"src/**/*.{js,ts,tsx}\" --quiet --fix",
    "format": "prettier ./**/*.{js,jsx,ts,tsx,css,scss,md,json} --config ./.prettierrc --write",
    "formatmd": "prettier --write",
    "success-message": "echo \n\n\nğŸš€  Everything is awesome. ğŸš€\n\nğŸ–¥ï¸  Auto checking completed, ready to deploy.ğŸ–¥ï¸\n\nâ˜ï¸  Run: git push â˜ï¸"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^12.1.6",
    "@reduxjs/toolkit": "^1.8.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-markdown": "^2.2.1",
    "highlight.js": "^11.5.1",
    "mongo": "^0.1.0",
    "mongodb": "^4.7.0",
    "next": "^12.1.6",
    "next-auth": "^4.6.1",
    "next-redux-wrapper": "^7.0.5",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-code-blocks": "^0.0.9-0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-redux": "^8.0.2",
    "react-syntax-highlighter": "^15.5.0",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "post-commit": "npm run success-message",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html}": [
      "npm run lint",
      "npm run format",
      "git add ."
    ],
    "*.{md}": [
      "npm run formatmd",
      "git add"
    ]
  },
  "commitlint": {
    "rules": {
      "references-empty": [
        2,
        "always"
      ]
    },
    "parserPreset": {
      "parserOpts": {
        "issuePrefixes": [
          "ISSUE-"
        ]
      }
    }
  }
}
